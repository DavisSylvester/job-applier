# Indeed Credentials
INDEED_EMAIL=your-email@example.com
INDEED_PASSWORD=your-password
INDEED_AUTH_PROVIDER=password # or 'google'

# Google OAuth (required if INDEED_AUTH_PROVIDER=google)
GOOGLE_EMAIL=your-google-email@example.com
GOOGLE_PASSWORD=your-google-password

# Proxy Configuration (Decodo Residential or similar)
PROXY_HOST=proxy.decodio.com
PROXY_PORT=8080
PROXY_USERNAME=
PROXY_PASSWORD=
USE_PROXY=true

# Job Search Configuration (defaults geared for NodeJS in Dallas/75495)
JOB_SEARCH_KEYWORDS=nodejs
JOB_LOCATION=Dallas, TX
JOB_RADIUS=100

# Use aggregator for Dallas/75495 NodeJS search (radius 100)
DECODO_DALLAS=false

# Application Configuration
AUTO_APPLY=false
DRY_RUN=true
MAX_APPLICATIONS_PER_DAY=10
HEADLESS=false

# Resume and Cover Letter
RESUME_PATH=./data/resume.pdf
COVER_LETTER_PATH=./data/cover-letter.txt
